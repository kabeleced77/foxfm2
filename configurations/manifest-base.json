{
    "manifest_version": 2,
    "name": "foxfm",
    "version": "1.0",
    "default_locale": "de",
	"options_ui": {
		"page": "settings.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-E8WV6VvuObH6nVPLMJ3PhAOt10VjZeatCqItFmhHmi8=' 'sha256-FkAEMQlvFMFUVr9kAPCNHRjpluxyMt2s898SncHW1+Y='; object-src 'self'",
    "permissions": [
        "contextMenus",
        "tabs",
        "storage"
    ],
    "background": {
        "page": "background.html"
    }
}
