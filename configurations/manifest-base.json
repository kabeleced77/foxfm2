{
  "manifest_version": 2,
  "name": "foxfm2",
  "version": "1.3.0",
  "default_locale": "de",
  "permissions": [
    "*://www.onlinefussballmanager.de/*",
    "*://en.onlinefootballmanager.com/*",
    "*://server2.onlinefussballmanager.de/*",
    "contextMenus",
    "tabs",
    "storage",
    "notifications"
  ],
  "icons": {
    "16": "foxfm16.png",
    "32": "foxfm32.png",
    "64": "foxfm64.png",
    "128": "foxfm128.png"
  },
  "background": {
    "scripts": [
      "jspm_packages/system-csp-production.src.js",
      "config.js",
      "dist/app-common-build.js",
      "dist/app-backgroundpage-build.js",
      "loadBackground.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "*://www.onlinefussballmanager.de/*",
        "*://en.onlinefootballmanager.com/*",
        "*://server2.onlinefussballmanager.de/*"
      ],
      "js": [
        "jspm_packages/system-csp-production.src.js",
        "config.js",
        "dist/app-common-build.js",
        "dist/app-contentscript-build.js",
        "loadContentScript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "*.png",
    "transfer/spielerwechsel_export.php"
  ],
  "options_ui": {
    "page": "settings.html"
  }
}
